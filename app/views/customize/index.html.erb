                <ol class="carousel-indicators">
                  <li data-target="#alipay_applet_carousel" data-slide-to="0" class="active"></li>
                  <li data-target="#alipay_applet_carousel" data-slide-to="1"></li>
                  <li data-target="#alipay_applet_carousel" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner">
                <%- (1..3).each do |i| -%>
